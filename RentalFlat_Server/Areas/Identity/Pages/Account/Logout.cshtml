@page

@inject SignInManager<IdentityUser> signInManager

@{
    ViewData["Title"] = "Log out";
}

@functions{
    public async Task<IActionResult> OnGet()
    {
        if (signInManager.IsSignedIn(User))
        {
            await signInManager.SignOutAsync();
        }

        return Redirect("~/");
    }
}